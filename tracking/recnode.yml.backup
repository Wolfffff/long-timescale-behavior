# uncomment the below for having the webinterface on the network port
#Common:
#  NetworkInterface: 'eno1'
Camera:
  Width: 1536
  OffsetX: 256
Record:
  #Directory: /mnt/DATA
  RecordCodecDefault: gpu-h264-hq
  RecordCodecs:
    cpu-h264-hq: libx264-crf10-uf
    cpu-h264-lq: libx264-crf23-mf
    cpu-h264-mq: libx264-crf20-uf
    gpu-hevc-hq: hevc-nvenc-slow-yuv420
    gpu-hevc-hq-fast: hevc-nvenc-fast-yuv420
    gpu-h264-hq: nvenc-slow-yuv420-gray
    gpu-h264-mq: nvenc-slow-yuv420-lq23-gray
    gpu-h264-lq: nvenc-slow-yuv420-lq30-gray
    gpu-h264-hq-fast: nvenc-medium-yuv420-gray
  RecordFPS: 120
  RecordToStore: user
  StoreChunkSize: 10000
  StoreCodecDefault: gpu-h264-hq
  StoreCodecs:
    cpu-h264-hq: libx264-crf10-uf
    cpu-h264-lq: libx264-crf23-mf
    cpu-h264-mq: libx264-crf20-uf
    gpu-hevc-hq: hevc-nvenc-slow-yuv420
    gpu-hevc-hq-fast: hevc-nvenc-fast-yuv420
    gpu-h264-hq: nvenc-slow-yuv420-gray
    gpu-h264-mq: nvenc-slow-yuv420-lq23-gray
    gpu-h264-lq: nvenc-slow-yuv420-lq30-gray
    gpu-h264-hq-fast: nvenc-medium-yuv420-gray
Camera:
    AcquisitionFrameRate: 120.0
    ExposureTime: 1000.0
    GPUId: 0
